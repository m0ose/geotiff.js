{
  "name": "geotiff",
  "version": "0.4.1",
  "description": "GeoTIFF image decoding in JavaScript",
  "repository": "https://github.com/constantinius/geotiff.js",
  "keywords": [
    "TIFF",
    "GeoTIFF",
    "image",
    "raster"
  ],
  "main": "dist/main.js",
  "dependencies": {
    "babel-cli": "^6.24.0",
    "grunt-cli": "^1.2.0",
    "pako": "^1.0.3",
    "xmldom": "0.1.*",
    "webworkify": "^1.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "budo": "^9.2.2",
    "chai": "^3.5.0",
    "es6-promise": "^4.0.5",
    "eslint": "^3.10.2",
    "eslint-config-pedant": "^0.8.0",
    "grunt": "^1.0.1",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-browserify": "^5.0.0",
    "grunt-bump": "^0.8.0",
    "grunt-concurrent": "^2.3.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-karma": "^2.0.0",
    "grunt-release": "^0.14.0",
    "jsdoc-babel": "^0.2.1",
    "jshint-stylish": "^2.2.1",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-source-map-support": "^1.2.0",
    "load-grunt-tasks": "^3.5.2",
    "mocha": "^3.1.2"
  },
  "scripts": {

    "watch": "watchify src/main.js -t babelify -o dist/geotiff.js -v",
    "dev": "budo src/main.js:dist/geotiff.js --live -o --dir test/ -- -t babelify",
    "build": "browserify src/main.js -d -x xmldom -t babelify -o dist/geotiff.js ; uglifyjs dist/geotiff.js -o dist/geotiff.min.js --source-map",
    "test": "grunt test",
    "lint": "eslint src",
    "docs": "rm -rf docs/*; jsdoc -c .jsdoc.json -r src README.md -d docs"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "es2017"
    ]
  },
  "author": "Fabian Schindler",
  "contributors": [
    {
      "name": "Fabian Schindler",
      "email": "fabian.schindler@eox.at"
    }
  ],
  "license": "MIT"
}
